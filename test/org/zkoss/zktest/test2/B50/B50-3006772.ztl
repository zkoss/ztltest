<test tags="B50-3006772.zul,tree,treeitem">
	<case id="focusArrow">
		<server id="zul"><![CDATA[
			<zk>
				<tree id="tree" multiple="true">
					<treechildren>
						<treeitem>
							<treerow>
								<treecell label="1" />
							</treerow>
							<treechildren>
								<treeitem>
									<treerow>
										<treecell label="1.1" />
									</treerow>
									<treechildren>
										<treeitem id="ti1">
											<treerow id="tr1">
												<treecell label="1.1.1" id="tc1"/>
											</treerow>
										</treeitem>
										<treeitem id="ti2">
											<treerow id="tr2">
												<treecell label="1.1.2" id="tc2"/>
											</treerow>
										</treeitem>
										<treeitem>
											<treerow>
												<treecell label="1.1.3" />
											</treerow>
										</treeitem>
									</treechildren>
								</treeitem>
							</treechildren>
						</treeitem>
					</treechildren>
				</tree>
			</zk>
		]]></server>
		<client><![CDATA[
			click(jq("@treecell:contains(1.1.1)"));
			click(jq("@treerow").toWidget().$n("icon"));
			click(jq("@treerow").toWidget().$n("icon"));
			click(jq("@treecell:contains(1.1.2)"));
			verifyEquals(1, jq("td.z-treerow-focus").length());
		]]></client>
	</case>
</test>