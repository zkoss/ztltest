<test tags="B50-3101558.zul,B,Combobox,bandbox,datebox,timebox,spinner,width">
	<case id="ztl">
		<server><!--
<zk>
 you shall see 5 "35px" widget in the screen ;
 <div id="div">
  <combobox width="35px" />
  <bandbox width="35px" />
  <datebox width="35px" />
  <timebox width="35px" />
  <spinner width="35px" />
 </div>
 
 
</zk>
		--></server>
		<client><![CDATA[
			JQuery widgets= jq("$div > *");
			for(int i =0 ,len = widgets.length() ; i< len;++i){
				int width = Integer.parseInt(widgets.eval("eq("+i+").width()"));
				verifyEquals(35,width);
			}
		]]></client>
	</case>
</test>