                <server><!--
					<zk>
						<div onClick="">
							<checkbox id="cb" label="click me"/>
						</div>
					</zk>
                --></server>
                <client>
                        click(cb.$n("real"));
                        verifyEquals("checked", jq(cb.$n("real")).is("[checked]"));
                </client>
        </case>
</test>